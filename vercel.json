{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/tienda-de-computadoras-y-accesorios"
      }
    }
  ],
  "routes": [
    {
      "src": "/(?:(?!.*\\.(?:js|css|map|json|svg|png|jpg|jpeg|ico)).*)",
      "dest": "/index.html"
    }
  ]
}
